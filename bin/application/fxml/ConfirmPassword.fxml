<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="240.0" prefWidth="360.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="application.gui.ConfirmPasswordController">
	<children>
		<BorderPane prefHeight="200.0" prefWidth="200.0"
			AnchorPane.bottomAnchor="24.0" AnchorPane.leftAnchor="24.0"
			AnchorPane.rightAnchor="24.0" AnchorPane.topAnchor="24.0">
			<center>
				<VBox BorderPane.alignment="CENTER">
					<children>
						<HBox>
							<children>
								<Label text="AccountID" />
								<Label text="-unknown" />
							</children>
							<padding>
								<Insets bottom="12.0" />
							</padding>
							<VBox.margin>
								<Insets top="12.0" />
							</VBox.margin>
						</HBox>
						<HBox>
							<children>
								<Label text="Mã nhân viên" />
								<Label text="-unknown" />
							</children>
							<padding>
								<Insets bottom="12.0" />
							</padding>
						</HBox>
						<Label text="Nhập mật khẩu" />
						<TextField />
						<Label text="Nhập lại mật khẩu">
							<padding>
								<Insets top="12.0" />
							</padding>
						</Label>
						<TextField />
					</children>
					<BorderPane.margin>
						<Insets />
					</BorderPane.margin>
				</VBox>
			</center>
			<top>
				<Label text="Tạo tài khoản" BorderPane.alignment="CENTER">
					<font>
						<Font name="System Bold" size="25.0" />
					</font>
				</Label>
			</top>
			<bottom>
				<HBox alignment="CENTER" BorderPane.alignment="CENTER">
					<children>
						<Button fx:id="btnOk" alignment="CENTER"
							mnemonicParsing="false" onAction="#CAction" text="Xác nhận">
							<font>
								<Font size="13.0" />
							</font>
						</Button>
						<Button fx:id="btnCancel" alignment="CENTER"
							mnemonicParsing="false" onAction="#CAction" text="Hủy">
							<font>
								<Font size="13.0" />
							</font>
						</Button>
					</children>
					<BorderPane.margin>
						<Insets top="12.0" />
					</BorderPane.margin>
				</HBox>
			</bottom>
		</BorderPane>
	</children>
</AnchorPane>
